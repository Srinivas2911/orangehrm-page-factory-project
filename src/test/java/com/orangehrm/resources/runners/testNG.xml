<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Orangehrm Test" verbose="1">
    <listeners>
        <listener class-name="com.orangehrm.listeners.Listeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="HomePage Test">
        <classes>
            <class name="com.orangehrm.testsuite.HomepageTest"/>
        </classes>
    </test>
    <test name="Login Test">
        <classes>
            <class name="com.orangehrm.testsuite.LoginTest"/>
        </classes>
    </test>

</suite>
